    // For showing a error or message to the user
    var _showMessageTimeout;

    function ShowUserMessage(message, isError, durration, skipDecode)
    {
        // Apply the good or bad background colors to the container
        var messageDiv = $("#header-message");
        messageDiv.removeClass("header-message-bad");
        messageDiv.removeClass("header-message-good");

        if (isError === true)
        {
            messageDiv.addClass("header-message-bad");
        }
        else {
            messageDiv.addClass("header-message-good");
        }

        // Set the message
        if (skipDecode !== true) {
            message = HtmlDecode(message);
        }
        $("#header-message-text").html(message);

        // Show the message display
        messageDiv.show();
        messageDiv.css("opacity", 0);
        messageDiv.css("height", 0);

        messageDiv.animate({
            opacity: 1.0,
            height: messageDiv.get(0).scrollHeight
        }, 300
        , function () {
            messageDiv.height("auto");
        });

        // Set a timer to hide the message
        if (durration > 0)
        {
            if (_showMessageTimeout)
            {
                clearTimeout(_showMessageTimeout);
            }

            _showMessageTimeout = window.setTimeout(function () {
                HideUserMessage();
            }, durration);
        }
    }

    function HideUserMessage()
    {
        var messageDiv = $("#header-message");

        if (_showMessageTimeout) {
            clearTimeout(_showMessageTimeout);
        }

        messageDiv.animate({
            opacity: 0.0,
            height: 0
        }, 300
        , function () {
            messageDiv.hide();
        });

        _showMessageTimeout = null;
    }

    function HtmlDecode(input)
    {
        return $("<div/>").html(input).text();
    }

