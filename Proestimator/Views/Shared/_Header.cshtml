<script>
    function RefreshHeaderInfo(estimateID) {

        $.getJSON("@Url.Action("GetEstimateHeaderInfo", "Estimate")", { userID: @ViewBag.UserID, estimateID: estimateID }, function (data) {

            $("#estimateNumbersmall").text(data.EstimateNumber);
            $("#estimateNumberLarge").text(data.EstimateNumber);

            $("#estimateStatusSmall").text(data.StatusName);
            $("#estimateStatusLarge").text(data.StatusName);

            $("#estimatePanelNameSmall").text($.trim(data.CustomerName));
            $("#estimatePanelNameLarge").text($.trim(data.CustomerName));

            $("#estimatePanelVehicleSmall").text($.trim(data.VehicleName));
            $("#estimatePanelVehicleLarge").text($.trim(data.VehicleName));

            if ($.trim(data.Vin).length == 17) {
                var estimatePanelVin = $.trim(data.Vin);
                var estimatePanelVinBeforeLast8Chars = estimatePanelVin.substr(0, 9);
                var estimatePanelVinLast8Chars = estimatePanelVin.substr(9, 8);
                $("#estimatePanelVinSmall").html(estimatePanelVinBeforeLast8Chars + "<span class='bold-vin'>" + estimatePanelVinLast8Chars + "</span>");
                $("#estimatePanelVinLarge").html(estimatePanelVinBeforeLast8Chars + "<span class='bold-vin'>" + estimatePanelVinLast8Chars + "</span>");
            }
            else {
                $("#estimatePanelVinSmall").text($.trim(data.Vin));
                $("#estimatePanelVinLarge").text($.trim(data.Vin));
            }

            $("#estiamtePanelCostsSmall").text(data.EstimateTotal);
            $("#estiamtePanelCostsLarge").text(data.EstimateTotal);

            if (data.IsTotalLoss)
            {
                $("#totalLossLarge").show();
                $("#totalLossSmall").show();
            }
            else
            {
                $("#totalLossLarge").hide();
                $("#totalLossSmall").hide();
            }

            if ($.trim(data.CustomerName) == "") {
                $("#dividerNameSmall").hide();
                $("#dividerNameLarge").hide();
            }
            else {
                $("#dividerNameSmall").show();
                $("#dividerNameLarge").show();
            }

            if ($.trim(data.VehicleName) == "") {
                $("#dividerVehicleSmall").hide();
                $("#dividerVehicleLarge").hide();
            }
            else {
                $("#dividerVehicleSmall").show();
                $("#dividerVehicleLarge").show();
            }

            if ($.trim(data.Vin) == "") {
                $("#dividerVinSmall").hide();
                $("#dividerVinLarge").hide();
            }
            else {
                $("#dividerVinSmall").show();
                $("#dividerVinLarge").show();
            }

            $(".header-detail-container").fadeTo("fast", 1);
        });
    }

